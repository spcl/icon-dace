prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
cflags=-I${prefix}/include
fcflags=@FPP_INCOPT@${prefix}/include @CDI_F90_INTERFACE_FCFLAGS@
libs=-L${exec_prefix}/lib -lcdipio     -lcdi    -lnetcdf   -lm  -leccodes -lnetcdff -lnetcdf -lopenblas -lradiation
threads_libs=
threads_cflags=
szlib_cflags=
szlib_libs=
netcdf_root=
netcdf_cflags=
netcdf_libs= -lnetcdf
grib_api_cflags=
grib_api_libs=
eccodes_cflags=
eccodes_libs=
yaxt_fc_include=
yaxt_fc_lib=
yaxt_c_cflags=
yaxt_c_libs=
ppm_core_cflags=
ppm_core_libs=
aux_libs= -lm  -leccodes -lnetcdff -lnetcdf -lopenblas -lradiation
cdipio_build_cc=mpicc
cdipio_build_cflags=-g -march=native -O2
cdipio_build_cppflags=-I/usr/include -I/usr/include/libxml2 
cdipio_build_ldflags=-L/usr/lib/x86_64-linux-gnu/ -L/home/primrose/Work/IconGrounds/icon-dace/.dacecache/radiation/build
cdipio_build_libs=-lm  -leccodes -lnetcdff -lnetcdf -lopenblas -lradiation
cdipio_build_fc=mpif90
cdipio_build_fcflags=-g -O2 -I/usr/include/ -Wall -frecursive -Wno-unused-variable -Wno-unused-dummy-argument -Wno-unused-function -Wno-missing-include-dirs -DDACE_SUBST_VERIFY -DDACE_SUBST_ENABLE -std=legacy
cdipio_build_f77=no
cdipio_build_fflags=
cdipio_build_cxx=no
